#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/run/media/zupeli/HD_DADOS/Github/zpFolio/node_modules/.pnpm/oxlint@1.42.0/node_modules/oxlint/bin/node_modules:/run/media/zupeli/HD_DADOS/Github/zpFolio/node_modules/.pnpm/oxlint@1.42.0/node_modules/oxlint/node_modules:/run/media/zupeli/HD_DADOS/Github/zpFolio/node_modules/.pnpm/oxlint@1.42.0/node_modules:/run/media/zupeli/HD_DADOS/Github/zpFolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/run/media/zupeli/HD_DADOS/Github/zpFolio/node_modules/.pnpm/oxlint@1.42.0/node_modules/oxlint/bin/node_modules:/run/media/zupeli/HD_DADOS/Github/zpFolio/node_modules/.pnpm/oxlint@1.42.0/node_modules/oxlint/node_modules:/run/media/zupeli/HD_DADOS/Github/zpFolio/node_modules/.pnpm/oxlint@1.42.0/node_modules:/run/media/zupeli/HD_DADOS/Github/zpFolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../oxlint/bin/oxlint" "$@"
else
  exec node  "$basedir/../oxlint/bin/oxlint" "$@"
fi
